pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Diploy') {
            steps {
                echo 'Diploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        
        stage('Reliase') {
            steps {
                echo 'Release Build'
            }
        }
    }
}
